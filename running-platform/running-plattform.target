<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?pde version="3.6"?>

<target name="at.bxm.running.platform">
<locations>
<location path="${project_loc}/bundles" type="Directory"/>
<location path="${eclipse_home}" type="Profile"/>
</locations>
<includeBundles/>
<environment>
<os>win32</os>
<ws>win32</ws>
<arch>x86</arch>
<nl>de_AT</nl>
</environment>
<launcherArgs>
<vmArgs>-Dosgi.requiredJavaVersion=1.5 -Xms40m -Xmx512m</vmArgs>
</launcherArgs>
</target>
